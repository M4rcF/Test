<head>
  <meta charset="utf-8"/>
</head>
<table>
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Age</th>
      <th scope="col">Cep</th>
      <th scope="col">Logradouro</th>
      <th scope="col">Complemento</th>
      <th scope="col">Bairro</th>
      <th scope="col">Localidade</th>
      <th scope="col">UF</th>
    </tr>
  </thead>
  <tbody>
    <% @people.each do |person| %>
        <tr>
            <td><%= person.name %></td>
            <td><%= person.age %></td>
            <td><%= person.address.cep %></td>
            <td><%= person.address.logradouro %></td>
            <td><%= person.address.complemento %></td>
            <td><%= person.address.bairro %></td>
            <td><%= person.address.localidade %></td>
            <td><%= person.address.uf %></td>
        </tr>
    <% end %>
  </tbody>
</table>